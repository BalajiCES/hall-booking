{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-unused-vars":"warn",
        "object-curly-newline": ["off", { "ObjectPattern": {"multiline": true }, "ImportDeclaration": "never", "ExportDeclaration": "never" }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "comma-dangle": ["error", "never"],
        "operator-linebreak": [2, "after"],
        "react/jsx-one-expression-per-line": ["off",{ "allow": "single-child" }]

    }
}
